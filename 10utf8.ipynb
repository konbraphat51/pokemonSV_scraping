   "source": [
    "import json\n",
    "\n",
    "#read json\n",
    "f = open(\"data/0pokemon.json\", \"r\")\n",
    "data = f.read()\n",
    "f.close()\n",
    "\n",
    "pokemons = json.loads(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "#保存\n",
    "tf = open(\"data/0pokemon-utf8.json\", \"w\", encoding=\"utf-8\")\n",
    "json.dump(pokemons, tf, ensure_ascii=True)\n",
    "tf.close()"
   ]
  }
